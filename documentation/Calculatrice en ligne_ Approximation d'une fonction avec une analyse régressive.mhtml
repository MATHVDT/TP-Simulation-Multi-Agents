	swapConsent( consPrev, consNext );
});
consNext.find('.consent-navigate').on("click", function() {
	swapConsent( consNext, consPrev );
});
function swapConsent( cHide, cShow ) {
	cHide.hide(500, function() {
		cShow.show( 500 );
	});
}
function setCookie(cname, cvalue, exdays) {
	    var d = new Date();
	   var consent = ""+ cvalue + ":" + Math.round(d.getTime()/1000);
	        d.setTime(d.getTime() + (exdays*24*60*60*1000));
